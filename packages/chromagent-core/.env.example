# Environment variables for Chromagent integration tests
# Copy this file to .env and fill in your actual API keys

# Vertex AI API key for Gemini models
VERTEX_GEMINI_API_KEY=your-vertex-gemini-api-key-here

# Optional: Vertex location (defaults to us-central1)
VERTEX_LOCATION=us-central1

# Optional: Vertex project ID (defaults to your-project-id)
VERTEX_PROJECT_ID=your-project-id-here